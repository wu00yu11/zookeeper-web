<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.learn.example.zookeeperweb.dao.UserDao">
    <resultMap id="zkCfgMap" type="ZkCfg">
        <result column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="host" property="host"/>
        <result column="desc" property="desc"/>
    </resultMap>

    <insert id="add" parameterType="com.learn.example.zookeeperweb.model.ZkCfg">
        insert into zk_cfg (name ,host,desc) values (#{name},#{host},#{desc})
    </insert>

    <select id="getAllUser" resultMap="userMap">
      select * from user_info
    </select>


</mapper>